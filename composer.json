{
  "name": "mm-ta/grasshopper",
  "description": "A Cli Race Game",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad Mahdi Taheri",
      "email": "mmtaheri.dev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-readline": "*",
    "wp-cli/php-cli-tools": "^0.11.22",
    "pimple/pimple": "^3.5"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.42",
    "pestphp/pest": "^2.35",
    "mockery/mockery": "^1.6",
    "symfony/var-dumper": "^7.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "pestphp/pest-plugin": true
    },
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  }
}
